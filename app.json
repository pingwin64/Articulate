{
  "expo": {
    "name": "Articulate",
    "slug": "Articulate",
    "version": "1.0.0",
    "description": "Read one word at a time. Build focus, fluency, and confidence with science-backed reading practice.",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "scheme": "articulate",
    "platforms": [
      "ios"
    ],
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "net.ordco.articulate",
      "buildNumber": "1",
      "deploymentTarget": "15.1",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      },
      "privacyManifests": {
        "NSPrivacyAccessedAPITypes": [
          {
            "NSPrivacyAccessedAPIType": "NSPrivacyAccessedAPICategoryUserDefaults",
            "NSPrivacyAccessedAPITypeReasons": [
              "CA92.1"
            ]
          }
        ],
        "NSPrivacyCollectedDataTypes": []
      }
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "plugins": [
      [
        "expo-router",
        {
          "root": "./src/app"
        }
      ],
      "expo-font",
      [
        "expo-notifications",
        {
          "icon": "./assets/icon.png",
          "color": "#000000"
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "Allow Articulate to access your photos to scan text from images.",
          "cameraPermission": "Allow Articulate to use your camera to scan text from photos."
        }
      ],
      [
        "expo-audio",
        {
          "microphonePermission": "Allow Articulate to use your microphone to practice pronunciation."
        }
      ],
      [
        "./plugins/withMetroPort",
        47000
      ],
      [
        "expo-dev-launcher",
        {
          "launchMode": "most-recent"
        }
      ]
    ],
    "extra": {
      "router": {
        "root": "./src/app"
      },
      "eas": {
        "projectId": "de8718f9-5174-4054-ba4b-6c66df98ba78"
      }
    }
  }
}
